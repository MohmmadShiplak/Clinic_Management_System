<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="ProjectName" value="Clinic Project" />

		<add key="MaxPersonAge" value="80" />
		<add key="MinPersonAge" value="10" />

		<add key="RowsPerPage" value="10" />

        <add key ="Working Days" value ="Sunday to Saturday"/>
		
   
		<add key ="holidays" value="Friday "/>
		

		<add key="ClinicOpeningTime" value="08:00 AM" />
		<add key="ClinicClosingTime" value="11:00 PM" />
	</appSettings>

	<connectionStrings>
		<add name="ConnectionString" connectionString="Server=.;Database=Clinic Project;Integrated Security=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>
		
 
	

 </configuration>
	
	
	
	
	
		